@model IEnumerable<CarWashFrontEnd.Models.CarViewModel>

@{
    ViewBag.Title = "CarDetails";
}

<h2>CarDetails</h2>
<style>
    body {
        background-image: url('https://img.freepik.com/free-vector/yellow-parchment_53876-91513.jpg?w=2000');
        background-position: inherit;
        background-repeat: no-repeat;
        background-size: cover;
    }
</style>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CarModel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CarModel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
          
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
